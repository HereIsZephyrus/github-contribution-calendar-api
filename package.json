    "twind": "^0.16.16",
    "worktop": "^0.7.1"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.5.10",
    "chokidar-cli": "^3.0.0",
    "prettier": "^2.3.2",
    "prettier-config-rschristian": "^0.1.1",
    "sirv-cli": "^1.0.12"
  },
  "prettier": "prettier-config-rschristian"
}
