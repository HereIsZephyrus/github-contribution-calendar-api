{
  "name": "github-contribution-calendar-api",
  "version": "1.0.0",
  "main": "build/github-contribution-calendar/index.js",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build:workers": "cfw build",
    "build:pages": "wmr build --prerender && node scripts/post-build-pages.js",
    "deploy": "cfw deploy",
    "dev:workers": "chokidar 'workers/api/*' --initial -c 'yarn build:workers' & miniflare build/github-contribution-calendar/index.js -p 3000 -w",
    "dev:pages": "wmr -p 3000",
    "format": "prettier --write --ignore-path .gitignore ."
  },
  "dependencies": {
    "@rschristian/twind-wmr": "^0.2.0",
    "@twind/preset-tailwind": "^1.0.0-next.38",
    "preact": "^10.7.1",
    "preact-iso": "^2.3.0",
    "preact-render-to-string": "^5.2.0",
    "prismjs": "^1.28.0",
    "twind": "^1.0.0-next.38",
    "wmr": "^3.7.2"
  },
  "devDependencies": {
    "@types/node": "^16",
    "cfw": "^0.3.0",
    "miniflare": "^1.3.3",
    "prettier": "^2.6.2",
    "prettier-config-rschristian": "^0.1.1",
    "typescript": "^4.6.4",
    "worktop": "^0.7.3"
  },
  "prettier": "prettier-config-rschristian"
}
